# Environment Variables Template
# Copy this file to .env and fill in your values

# Database Configuration
# For local development: postgresql://user:password@localhost:5432/dbname
# For Docker: postgresql://user:password@db:5432/dbname
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/kpi_db

# Admin Configuration
ADMIN_EMAIL=admin@neemba.com
ADMIN_PASSWORD=your_admin_password_here

# Environment (dev, staging, prod)
ENV=dev
